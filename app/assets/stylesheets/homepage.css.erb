body {
  background-image: url(<%= asset_path('blur.jpg') %>);
  background-repeat: no-repeat;
}


.navigation {
  padding: 30px;
}

.navigation .links {
  float:right;
}

h2.headline {
  text-align: center;
}

.top .navigation a,
.top .navigation a:visited,
.top .navigation a:active {
  color: #FFF;
}

.top .navigation a:hover {
  color:#000;
}

.top {
  margin-bottom: 50px;
}

.container.header {
  color:#FFF;
}

.odd-strip {
  background-color: #F6F6F6;

}

.container.strip {
  margin: 20px auto;
  /*padding: 20px;*/
}

.main-content {
  background-color: #FFF;
  padding:40px 0px;
}

img.window-one {
  background-image: url(<%= asset_path('homepage/landing-pages.png')%>);
  background-position: 0px 20px;
  background-repeat: no-repeat;
}

img.window-three {
  background-image: url(<%= asset_path('homepage/landing-split.png')%>);
  background-position: 0px 20px;
  background-repeat: no-repeat;
}
